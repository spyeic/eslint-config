{
    "type": "module",
    "name": "eslint-config-spyeic",
    "version": "1.0.3",
    "description": "spyeic's eslint config",
    "main": "./dist/cjs/common.cjs",
    "module": "./dist/esm/common.mjs",
    "exports": {
        ".": {
            "import": "./dist/esm/common.mjs",
            "require": "./dist/cjs/common.cjs",
            "default": "./dist/esm/common.cjs"
        },
        "./ts": {
            "import": "./dist/esm/ts.mjs",
            "require": "./dist/cjs/ts.cjs",
            "default": "./dist/esm/ts.cjs"
        }
    },
    "scripts": {
        "build": "rollup -c"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/spyeic/eslint-config.git"
    },
    "keywords": [
        "eslint",
        "config",
        "spyeic"
    ],
    "author": "spyeic",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/spyeic/eslint-config/issues"
    },
    "homepage": "https://github.com/spyeic/eslint-config#readme",
    "peerDependencies": {
        "eslint": ">=8",
        "eslint-plugin-import": ">=2",
        "@typescript-eslint/eslint-plugin": ">=6",
        "@typescript-eslint/parser": ">=6"
    },
    "peerDependenciesMeta": {
        "@typescript-eslint/eslint-plugin": {
            "optional": true
        },
        "@typescript-eslint/parser": {
            "optional": true
        }
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^25.0.7",
        "@typescript-eslint/eslint-plugin": "^6.10.0",
        "@typescript-eslint/parser": "^6.10.0",
        "eslint": "^8.53.0",
        "eslint-plugin-import": "^2.29.0",
        "rollup": "^4.4.0",
        "typescript": "^5.2.2"
    }
}
